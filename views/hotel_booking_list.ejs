<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Hotel Bookings List</title>
 

</head>
<body>
    <%-include("partials/header.ejs")%>
    <br>
    <br>
    <div class="container">
            <h3>Hotel Bookings List</h3>
            <table class="table">
                <thead>
                    <tr>
                        <th scope="col">Hotel Booking Id</th>
                        <th scope="col">Customer Name</th>
                        <th scope="col">Hotel Name</th>
                        <th scope="col">Hotel Location</th>
                        <th scope="col">Hotel Nights</th>
                        <th scope="col">Hotel Check In Date</th>
                        <th scope="col">Hotel Check Out Date</th>
                        <th scope="col">Hotel Quantity</th>
                        <th scope="col">Hotel Rate</th>
                        <th scope="col">Hotel SAC Code</th>
                    </tr>
                </thead>
                <tbody>
                    
                    <% hotel_booking.forEach(function(booking) { %>
                        <tr>
                            
                            <td><%=booking.hotel_booking_id%></td>
                            <td><%=booking.hotel_booking_customer_name%></td>
                            <td><%=booking.hotel_name%></td>
                            <td><%=booking.hotel_location%></td>
                            <td><%=booking.hotel_nights%></td>
                            <td><%=booking.hotel_check_in_date%></td>
                            <td><%=booking.hotel_check_out_date%></td>
                            <td><%=booking.hotel_quantity%></td>
                            <td><%=booking.hotel_rate%></td>
                            <td><%=booking.hotel_sac_code%></td>
                        </tr>
                    <%});%>
                </tbody>
            </table>
            <br>
            
        <br>
        <br>

    </div> 
</body>

</html>